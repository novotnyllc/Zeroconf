<Page
    x:Class="ZeroconfTest.RT.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ZeroconfTest.RT"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel Orientation="Vertical">
            <Button Content="Resolve" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="100" Click="Resolve_Click"/>
            <Button Content="Browse" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="100" Click="Browse_Click"/>
            <Button Content="Cancel" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="100" Click="Cancel_Click"/>
        </StackPanel>
    </Grid>
</Page>
