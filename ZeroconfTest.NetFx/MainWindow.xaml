<Window x:Class="ZeroconfTest.NetFx.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" >
    <Grid>

        <StackPanel Orientation="Vertical">
            <Button Content="Resolve" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="75" Click="Resolve_Click"/>
            <Button Content="Browse" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="75" Click="Browse_Click"/>
            <Button Content="Cancel" HorizontalAlignment="Left" Margin="20" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        </StackPanel>

    </Grid>
</Window>
